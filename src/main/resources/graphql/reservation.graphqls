type Reservation {
    id: ID!
    association: Association!
    startDate: String!
    endDate: String!
    title: String!
    description: String!
    status: ReservationStatus
    maxSize: Int!
    users: [User]!
    tracks: [Track]!
    allowedWeaponTypes: [WeaponType]!
    reservationSerie: ReservationSeries
}

enum ReservationStatus {
    IDK
    # Voeg hier de andere enum waarden toe
}

type ReservationSeries {
