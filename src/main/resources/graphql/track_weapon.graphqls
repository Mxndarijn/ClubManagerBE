type Track {
    id: ID!
    name: String
    description: String
    association: Association
    allowedWeaponTypes: [WeaponType]
}

type Weapon {
    id: ID!
    name: String
    type: WeaponType
    status: WeaponStatus
}

enum WeaponStatus {
    ACTIVE
    INACTIVE
    MAINTENANCE
}

type WeaponType {
    id: ID!
    name: String
}
